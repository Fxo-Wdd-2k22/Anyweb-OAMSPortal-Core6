@section Scripts {

    <script>
        $('#btnPrintSummary').on('click', function () {
            window.location = '/Application/DownloadFeeVoucher?ApplicationId=@Model.ApplicationId';
        });
        function printData() {
            $("#printTable").css("display", "block");
            var divToPrint = document.getElementById("printTable");
            newWin = window.open("");
            newWin.document.write(divToPrint.outerHTML);
            newWin.print();
            $("#printTable").css("display", "none");
            newWin.close();
        }
        function Back() {
            window.location = '/Application/ViewProofRead?ApplicationId=@Model.ApplicationId';
        }
    </script>
     @if (Model.Approved == true)
    {
        <script>

            $(document).ready(function () {
                $(".progress-bar").attr('style', 'width:100% !important');
                $(".skill-percent").html("100%");
            });


        </script>
    }
    else
    {
        <script>

            $(document).ready(function () {
                $(".progress-bar").attr('style', 'width:75% !important');
                $(".skill-percent").html("75%");
            });


        </script>
    }
    <script type="text/javascript">

        $.validator.unobtrusive.parse('#basic');

        $(function () {

         

            $('.custom-file-input').on("change", function () {
                var fileApplicantName = $(this).val().split("\\").pop();
                $(this).next('.custom-file-label').html(fileApplicantName);
            });
        });

        function OnBegin() {
            $('.btnUpdate').html(`<i class='fa fa-spinner fa-spin'></i> Processing`);
        }
        function OnBasicInfoSuccess(res) {
            debugger;
            if (res.response == "Success") {
                window.location = '/home/welcome';
                $('.message').css('display', 'block');
                $(".linkFeaturedImage1").attr("src", '/images/Documents/' + res.imgsrc);
                ShowToast();



            }
            else {
                ShowErrorToast();

            }
        }
        function ShowToast() {
            var x = document.getElementById("snackbar");
            x.className = "show";
            setTimeout(function () { x.className = x.className.replace("show", ""); }, 4000);
        }
        function ShowErrorToast() {
            var x = document.getElementById("errorbar");
            x.className = "show";
            setTimeout(function () { x.className = x.className.replace("show", ""); }, 4000);
        }
    </script>



