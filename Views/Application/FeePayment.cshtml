                    <ul class="d-md-flex justify-content-center align-items-center">
                        <li class="h-100">
                            <p class="my-auto">
                                Applicant Name
                                <span> @Model.Applicants?.FirstOrDefault(x => x.ApplicantId == Model.ApplicantId)?.ApplicantName</span>
                            </p>
                        </li>
                        <li class="h-100">
                            <p class="my-auto">
                                Class
